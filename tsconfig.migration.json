{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "baseUrl": ".",
    "emitDecoratorMetadata": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "lib": ["ESNext"],
    "module": "ESNext",
    "moduleResolution": "node",
    "skipDefaultLibCheck": true,
    "skipLibCheck": true,
    "target": "ESNext",
    "types": ["node"]
  },
  "include": [
    "db/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
