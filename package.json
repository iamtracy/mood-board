  "scripts": {
    "nx": "npx nx",
    "predev": "./setup_db.sh start",
    "dev": "npx nx run-many --target=serve",
    "build": "nx run-many --target=build",
    "test:unit:ci": "nx run-many --target=test --coverage --ci",
    "lint": "nx run-many --target=lint",
    "test:unit": "nx affected:test --all --parallel --watch",
    "migration": "typeorm-ts-node-esm",
    "migration:generate": "npm run migration migration:generate -- -d ./db/typeorm.config.ts",
    "migration:migrate": "npm run migration migration:run  -d ./db/typeorm.config.ts",
    "migration:revert": "npm run migration migration:revert  -d ./db/typeorm.config.ts",
    "migration:drop": "npm run migration schema:drop  -d ./db/typeorm.config.ts",
    "migration:show": "npm run migration schema:drop -d ./db/typeorm.config.ts"
  },
  "private": true,
